{
  "compilerOptions": {
    "target": "ES2020",
    "module": "esnext",
    "moduleResolution": "bundler",
    "outDir": "dist-tmp",
    "rootDir": "src/main",
    "strict": true,
    "noImplicitAny": true,
    "lib": ["ES2020", "DOM"],
    "skipLibCheck": true
  },
  "include": ["src/main/**/*.ts", "src/main/index.ts"]
}
